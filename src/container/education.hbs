<main>
  {{#with main}}
  <section>
    {{>title data="Education" big=true}}
    <div>
      <ul>
        {{#each educlist}}
        <li>
          {{name}}
          {{#unless isEnd }} (not finished yet) {{/unless}}
        </li>

        {{/each}}
      </ul>
    </div>
  </section>
  <section>
    {{>title data="My Certificates"}}
    <div>
      <ul>
        {{#each certificates}}
        <li>
          {{name}}
          ({{id}})
        </li>
        {{/each}}
      </ul>
    </div>
  </section>
  {{/with}}
</main>